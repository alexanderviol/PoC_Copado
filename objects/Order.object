    <fields>
        <fullName>division_mdrf__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
AND(
brady_units__c &gt; diagnostic_units__c, 
brady_units__c &gt; tachy_units__c),
&quot;Bradycardia&quot;,

IF(
AND(
tachy_units__c &gt; brady_units__c,
tachy_units__c &gt; diagnostic_units__c),
&quot;Tachycardia&quot;,

IF(
cardiomessenger_units__c &gt; 0,
&quot;Bradycardia&quot;,

IF(
AND(
diagnostic_units__c &gt; brady_units__c, 
diagnostic_units__c &gt; tachy_units__c),
&quot;Diagnostic&quot;,

IF(
AND(
tachy_leads__c = 0,
brady_leads__c = 0),
&quot;Bradycardia&quot;,

IF(
tachy_leads__c &gt; 0,
&quot;Tachycardia&quot;,

IF(
brady_leads__c &gt; 0,
&quot;Bradycardia&quot;,

&quot;Diagnostic&quot;)))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Division (MDRF)</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>division_number_mdrf__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
AND(
brady_units__c &gt; diagnostic_units__c, 
brady_units__c &gt; tachy_units__c),
&quot;10&quot;,

IF(
AND(
tachy_units__c &gt; brady_units__c,
tachy_units__c &gt; diagnostic_units__c),
&quot;20&quot;,

IF(
cardiomessenger_units__c &gt; 0,
&quot;10&quot;,

IF(
AND(
diagnostic_units__c &gt; brady_units__c, 
diagnostic_units__c &gt; tachy_units__c),
&quot;05&quot;,

IF( 
AND( 
tachy_leads__c = 0, 
brady_leads__c = 0), 
&quot;10&quot;,

IF( 
tachy_leads__c &gt; 0, 
&quot;20&quot;, 

IF( 
brady_leads__c &gt; 0, 
&quot;10&quot;, 

&quot;05&quot;)))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Division Number (MDRF)</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
