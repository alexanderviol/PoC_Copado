    <validationRules>
        <fullName>CHECK_ROLE_FOR_CANCEL_MDRF_ORDER</fullName>
        <active>true</active>
        <description>User with OPs or CS role are allowed to Cancel an order but in additional CS CAN&apos;T CANCEL IMPLANT ORDER.</description>
        <errorConditionFormula>AND(
Not($Permission.Validationrule_Exception),
$UserRole.DeveloperName = &quot;BINC_Customer_Solutions&quot;,

ISCHANGED(Status), 
ISPICKVAL(Status, &quot;Canceled&quot;), 
PRIORVALUE(Status) != &quot;Canceled&quot;,
 
RecordType.DeveloperName = &quot;mdrf_order&quot; 
)</errorConditionFormula>
        <errorMessage>You are not allowed to cancel an implant order.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CM_field_updates_not_allowed</fullName>
        <active>true</active>
        <description>SAFBIO-644  // Only CS/CAS is allowed to change the PO Number or Shipping Adress in the status after &quot;Not submitted&quot;</description>
        <errorConditionFormula>AND(

  Not($Permission.Validationrule_Exception),
  CASE($UserRole.DeveloperName,
     &quot;BINC_Customer_Solutions&quot;,1,
     &quot;BINC_Sales_Operations&quot;,1, 0 )
    = 0,
  RecordType.DeveloperName == &apos;cardio_messenger_order&apos;,
  AND(
   NOT(ISCHANGED(Status)),
   NOT(ISCHANGED(PoNumber)),
   NOT(ISCHANGED(ShippingCity)),
   NOT(ISCHANGED(ShippingStreet)),
   NOT(ISCHANGED(ShippingCountry)),
   NOT(ISCHANGED(ShippingPostalCode)),
   NOT(ISCHANGED(ShippingState)),
   NOT(ISCHANGED(date_cm_approved__c)), 
   NOT(ISCHANGED(submitted_date__c )), 
   NOT(ISCHANGED(pricebook__c)), 
   NOT(ISCHANGED(Pricebook2Id)) 
   ),
  OR(
    ISPICKVAL(Status , &quot;Processed to SAP&quot;),
    ISPICKVAL(Status , &quot;Invoiced&quot;),
    ISPICKVAL(Status , &quot;Shipped&quot;)
  )
)</errorConditionFormula>
        <errorMessage>Your role does not allow these field changes</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_role_for_status_change</fullName>
        <active>true</active>
        <description>SAFBIO-915 // Prevent users to change to certain status values</description>
        <errorConditionFormula>OR(

AND(
  !ISNEW(),

  Not($Permission.Validationrule_Exception),
  CASE($UserRole.DeveloperName,
    &quot;BINC_Customer_Solutions&quot;,1,
    &quot;BINC_Sales_Operations&quot;,1, 0 )
  = 0,
  PRIORVALUE(Status)=&quot;Not Submitted&quot;,
  OR(ISPICKVAL(Status,&quot;Invoiced&quot;),
    ISPICKVAL(Status,&quot;Pending Review&quot;),
    ISPICKVAL(Status,&quot;Pending SAP Submission&quot;),
    ISPICKVAL(Status,&quot;SAP Error Review&quot;),
    ISPICKVAL(Status,&quot;Processed to SAP&quot;),
    ISPICKVAL(Status,&quot;SAP Submission Blocked&quot;)
  ), 
  ISCHANGED(Status)
),

AND(
  !ISNEW(),

  Not($Permission.Validationrule_Exception),
  CASE($UserRole.DeveloperName,
    &quot;BINC_Customer_Solutions&quot;,1,
    &quot;BINC_Sales_Operations&quot;,1, 0 )
  = 0,
  OR(PRIORVALUE(Status)=&quot;Invoiced&quot;,
    PRIORVALUE(Status)=&quot;Pending Review&quot;,
    PRIORVALUE(Status)=&quot;Pending SAP Submission&quot;,
    PRIORVALUE(Status)=&quot;SAP Error Review&quot;,
    PRIORVALUE(Status)=&quot;Processed to SAP&quot;,
    PRIORVALUE(Status)=&quot;SAP Submission Blocked&quot;
  ),
  ISCHANGED(Status)
),

AND(
  !ISNEW(),
  $Profile.Name = &apos;HMSC&apos;,
  PRIORVALUE(Status)!=&quot;Pending HMSC Registration&quot;,
  ISCHANGED(Status)
)
)</errorConditionFormula>
        <errorMessage>You are not allowed to change the status to this value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MDRF_field_updates_not_allowed</fullName>
        <active>false</active>
        <description>Only CAS and CS are allowd to make certain field updates during status changes // SAFBIO-914</description>
        <errorConditionFormula>AND(
 RecordType.DeveloperName = &apos;mdrf_order&apos;,

 Not($Permission.Validationrule_Exception),
 CASE($UserRole.DeveloperName,
   &quot;BINC_Customer_Solutions&quot;,1,
   &quot;BINC_Sales_Operations&quot;,1, 0 )
   = 0,
 AND(
   NOT(ISCHANGED(Status)),
   NOT(ISCHANGED(date_cm_approved__c)),
   NOT(ISCHANGED(PoNumber)),
   NOT(ISCHANGED(submitted_date__c )),
   NOT(ISCHANGED(pricebook__c)),
   NOT(ISCHANGED(Pricebook2Id))
 )
)</errorConditionFormula>
        <errorMessage>Your role does not allow these changes in this status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ONLY_OWNER_CANCEL_ORDER</fullName>
        <active>true</active>
        <description>Only the owner of a record can cancel an order.</description>
        <errorConditionFormula>AND(
Not($Permission.Validationrule_Exception),
$Profile.Name = &quot;Sales&quot;, 

ISCHANGED(Status), 
ISPICKVAL(Status, &quot;Canceled&quot;), 
PRIORVALUE(Status) != &quot;Canceled&quot;, 
CASE($User.Id, OwnerId ,1 ,CreatedBy.Id, 1,0) = 0 
)</errorConditionFormula>
        <errorMessage>You are not allowed to cancel this order because you are not the owner or the creator.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Order_Created_Date_is_read_Only</fullName>
        <active>true</active>
        <description>Created date should not be an editable field.</description>
        <errorConditionFormula>AND(
ISCHANGED(EffectiveDate), 
NOT(ISNEW()),
Not($Permission.Validationrule_Exception))</errorConditionFormula>
        <errorMessage>You are not allowed to change the Created date!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PATIENT_REQUIRED_EXCEPT_FOR_CANCELLATION</fullName>
        <active>true</active>
        <description>Patient should not be required on MDRF Page Layout but Validation has to be created for MDRF Orders, that Patient Value is required for specific Order Status (all Status except Canceled).</description>
        <errorConditionFormula>AND(
ISBLANK(patient__c),
NOT(ISPICKVAL(Status ,&quot;Canceled&quot;))
)</errorConditionFormula>
        <errorDisplayField>patient__c</errorDisplayField>
        <errorMessage>Please enter a patient.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rep_at_implant_empty_when_no_rep_present</fullName>
        <active>true</active>
        <description>If &quot;No Rep present at implant&quot; is checked, the &apos;Rep at Implant&apos; must be empty and vice versa.</description>
        <errorConditionFormula>($RecordType.DeveloperName = &quot;mdrf_order&quot;)
&amp;&amp;
((ISBLANK(rep_at_implant__c) &amp;&amp; (no_rep_present_at_implant__c = FALSE)) ||
(NOT(ISBLANK(rep_at_implant__c)) &amp;&amp; (no_rep_present_at_implant__c = TRUE)))</errorConditionFormula>
        <errorDisplayField>rep_at_implant__c</errorDisplayField>
        <errorMessage>Select either &quot;Rep at Implant&quot; or &quot;No Rep present at implant&quot;</errorMessage>
    </validationRules>
