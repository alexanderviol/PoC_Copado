    <fields>
        <fullName>email_matches_user_s_email__c</fullName>
        <description>Used for reporting to catch Employee Contact records that have an email that doesn&apos;t match their User record.</description>
        <externalId>false</externalId>
        <formula>IF(
RecordType.DeveloperName != &quot;X5500_Employee_Contact&quot;,
&quot;n/a&quot;,

IF(
AND(
RecordType.DeveloperName = &quot;X5500_Employee_Contact&quot;,
ISBLANK(user__c)),
&quot;No User Associated to Employee Contact&quot;,

IF(
AND(
RecordType.DeveloperName = &quot;X5500_Employee_Contact&quot;,
Email = user__r.Email),
&quot;Emails Match&quot;,

IF(
AND(
RecordType.DeveloperName = &quot;X5500_Employee_Contact&quot;,
Email != user__r.Email),
&quot;Emails Do NOT Match&quot;,

&quot;Unknown&quot;))))</formula>
        <label>Email Matches User&apos;s Email</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
