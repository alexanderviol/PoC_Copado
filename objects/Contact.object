    <validationRules>
        <fullName>Capitalize_letter_for_first_name</fullName>
        <active>false</active>
        <description>This validation rule avoid the creation of the first name of the contact when it does not start with an upper case.</description>
        <errorConditionFormula>AND (
  Not($Permission.Validationrule_Exception),
  NOT(REGEX(FirstName, &quot;\\p{Lu}.*&quot;))
)</errorConditionFormula>
        <errorDisplayField>FirstName</errorDisplayField>
        <errorMessage>Please use capitalize letter for First Name</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contact_Name</fullName>
        <active>false</active>
        <description>Field checks wheter the Last Name starts with a capital letter</description>
        <errorConditionFormula>AND (
  Not($Permission.Validationrule_Exception),
  NOT(REGEX(LastName, &quot;\\p{Lu}.*&quot;))
)</errorConditionFormula>
        <errorDisplayField>LastName</errorDisplayField>
        <errorMessage>Please use capitalize letter for Last Name</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_of_Death_Not_In_Future</fullName>
        <active>true</active>
        <description>Prevents users from entering a Date of Death in the future.</description>
        <errorConditionFormula>date_of_death__c &gt; TODAY()</errorConditionFormula>
        <errorDisplayField>date_of_death__c</errorDisplayField>
        <errorMessage>Please enter a date of death that is not in the future.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_of_Death_required_if_deceased</fullName>
        <active>true</active>
        <description>Either &quot;Date of Death&quot; or &quot;Date of Death Unknown&quot; must be filled in by the user when Patient status &quot;Deceased&quot; is selected.</description>
        <errorConditionFormula>AND(
RecordType.DeveloperName=&quot;us_patient&quot;,
TEXT(PRIORVALUE(Status__c))!=&quot;Deceased&quot;,
ISPICKVAL(Status__c,&quot;Deceased&quot;),
date_of_death_unknown__c=False,
ISBLANK(date_of_death__c))</errorConditionFormula>
        <errorDisplayField>date_of_death__c</errorDisplayField>
        <errorMessage>Please enter either value for &quot;Date of Death&quot; or select &quot;Date of Death unknown&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_of_birth_not_in_future</fullName>
        <active>true</active>
        <description>Field checks wheter the birthdate is in the past.</description>
        <errorConditionFormula>Birthdate  &gt;= TODAY()</errorConditionFormula>
        <errorDisplayField>Birthdate</errorDisplayField>
        <errorMessage>Please insert a birthdate in the past.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_of_birth_required</fullName>
        <active>true</active>
        <description>Field checks wheter a birthdate is filled in</description>
        <errorConditionFormula>$RecordType.DeveloperName = &quot;us_patient&quot;
&amp;&amp;
(
	(
		ISBLANK(Birthdate)
		&amp;&amp;
		declined_to_provide_dob__c  == FALSE
	)
	||
	(
		NOT(ISBLANK(Birthdate))
		&amp;&amp;
		declined_to_provide_dob__c  == TRUE
	)
)</errorConditionFormula>
        <errorDisplayField>Birthdate</errorDisplayField>
        <errorMessage>Please either fill in &quot;Birthdate&quot; or check &quot;Declined to Provide&quot;, if the patient does not want to provide this information.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>First_name_required</fullName>
        <active>true</active>
        <description>This validation rule is defined to make the first name required.</description>
        <errorConditionFormula>AND (
  Not($Permission.Validationrule_Exception),
  ISBLANK(FirstName)
)</errorConditionFormula>
        <errorDisplayField>FirstName</errorDisplayField>
        <errorMessage>Complete this field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Graduation_Year_must_be_19xx_or_20xx</fullName>
        <active>true</active>
        <description>The Graduation Year must start with either 19 or 20, ie 1981 or 2014.</description>
        <errorConditionFormula>AND(
!ISBLANK(graduation_year__c),
LEFT(graduation_year__c,2) != &quot;19&quot;,
