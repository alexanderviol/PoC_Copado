    <validationRules>
        <fullName>SSN_Format</fullName>
        <active>true</active>
        <description>Inserted SSN Should be  without dashes OR is a format XXXXX + 4 digits.
The first three digits cannot be 000, 666, or anything 900-999
The fourth through fifth digits will never be 00
the sixth through ninth digits will never be 0000</description>
        <errorConditionFormula>$RecordType.DeveloperName = &quot;us_patient&quot;
&amp;&amp;
(
NOT(
OR(
ISBLANK(ssn__c),
REGEX( ssn__c, &quot;[0-9]{9}&quot;),
REGEX( ssn__c, &quot;(X{5})+([0-9]{4})&quot;)
)
)
)</errorConditionFormula>
        <errorDisplayField>ssn__c</errorDisplayField>
        <errorMessage>Please enter the right SSN with 9 digits or XXXXX + 4 digits</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SSN_Format_02</fullName>
        <active>true</active>
        <errorConditionFormula>$RecordType.DeveloperName = &quot;us_patient&quot;
&amp;&amp;
(
OR(
REGEX( ssn__c, &quot;[0-9]{3}+[0-9]{2}+0{4}|[0-9]{3}+0{2}+[0-9]{4}|0{3}+[0-9]{2}+[0-9]{4}|6{3}+[0-9]{2}+[0-9]{4}|0{9}&quot;),
REGEX( ssn__c, &quot;^(9[0-9]{0,2})+[0-9]{2}+[0-9]{4}$&quot;)
)
)</errorConditionFormula>
        <errorDisplayField>ssn__c</errorDisplayField>
        <errorMessage>SSN cannot start with 000, 666, or any number between 900-999.
The fourth through fifth digits cannot be 00.
The sixth through ninth digits cannot be 0000.</errorMessage>
    </validationRules>
