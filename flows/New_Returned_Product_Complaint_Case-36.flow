AND(
!ISBLANK({!Returned_Product_Sender_Name}),
{!A_hospital_clinic_place_sent_the_product_not_a_specific_person} = False,
{!I_don_t_know_who_returned_the_product} = False),

AND(
ISBLANK({!Returned_Product_Sender_Name}),
{!A_hospital_clinic_place_sent_the_product_not_a_specific_person} = True,
{!I_don_t_know_who_returned_the_product} = False),

AND(
ISBLANK({!Returned_Product_Sender_Name}),
{!A_hospital_clinic_place_sent_the_product_not_a_specific_person} = False,
{!I_don_t_know_who_returned_the_product} = True))</formulaExpression>
