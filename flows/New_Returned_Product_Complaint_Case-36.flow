        </connector>
        <fields>
            <name>IntroText</name>
            <fieldText>Please enter the following details for the product that was returned:</fieldText>
            <fieldType>DisplayText</fieldType>
        </fields>
        <fields>
            <name>screenSerialNumber</name>
            <fieldText>Serial Number: {!varAssetSerialNumber}</fieldText>
            <fieldType>DisplayText</fieldType>
        </fields>
        <fields>
            <name>Report_Date</name>
            <dataType>Date</dataType>
            <defaultValue>
                <elementReference>$Flow.CurrentDate</elementReference>
            </defaultValue>
            <fieldText>Date Product Received</fieldText>
            <fieldType>InputField</fieldType>
            <isRequired>true</isRequired>
            <validationRule>
                <errorMessage>The Date Product Received must be after both the Out of Service Date and Explant Date.</errorMessage>
                <formulaExpression>OR(

AND(
OR(
ISBLANK({!Out_of_Service_Date}),
{!Report_Date} &gt; {!Out_of_Service_Date}),
OR(
ISBLANK({!Explant_Date}),
{!Report_Date} &gt; {!Explant_Date}),

{!Report_Date} &lt;= {!$Flow.CurrentDate}))</formulaExpression>
            </validationRule>
        </fields>
        <fields>
            <name>Event_Date</name>
            <dataType>Date</dataType>
            <fieldText>Event Date</fieldText>
            <fieldType>InputField</fieldType>
            <isRequired>false</isRequired>
            <validationRule>
                <errorMessage>The Event Date must be before or equal to today&apos;s date.</errorMessage>
                <formulaExpression>OR(
ISBLANK({!Event_Date}),
{!Event_Date} &lt;= {!$Flow.CurrentDate})</formulaExpression>
            </validationRule>
        </fields>
        <fields>
            <name>Event_Reason</name>
            <choiceReferences>choiceERI</choiceReferences>
            <choiceReferences>choicePatient_Anatomy</choiceReferences>
            <choiceReferences>choicePatient_Death</choiceReferences>
            <choiceReferences>choiceUpgrade</choiceReferences>
            <choiceReferences>choiceOther</choiceReferences>
            <dataType>String</dataType>
            <fieldText>Event Reason</fieldText>
            <fieldType>DropdownBox</fieldType>
            <isRequired>false</isRequired>
        </fields>
        <fields>
            <name>Event_Description</name>
            <fieldText>Event Description</fieldText>
            <fieldType>LargeTextArea</fieldType>
            <isRequired>false</isRequired>
        </fields>
        <fields>
            <name>Out_of_Service_Date</name>
            <dataType>Date</dataType>
            <defaultValue>
                <elementReference>varOOSDate</elementReference>
            </defaultValue>
            <fieldText>Out of Service Date</fieldText>
            <fieldType>InputField</fieldType>
            <isRequired>false</isRequired>
            <validationRule>
                <errorMessage>The Out of Service Date must be before or equal to today&apos;s date and must be before or equal to Date Product Received.</errorMessage>
                <formulaExpression>OR(
ISBLANK({!Out_of_Service_Date}),
AND(
{!Out_of_Service_Date} &lt;= {!$Flow.CurrentDate},
{!Out_of_Service_Date} &lt;= {!Report_Date}))</formulaExpression>
            </validationRule>
        </fields>
        <fields>
            <name>Explant_Date</name>
            <dataType>Date</dataType>
            <defaultValue>
                <elementReference>varExplantDate</elementReference>
            </defaultValue>
            <fieldText>Explant Date</fieldText>
            <fieldType>InputField</fieldType>
            <isRequired>false</isRequired>
            <validationRule>
                <errorMessage>The Explant Date must be before or equal to today&apos;s date and must be before or equal to Date Product Received.</errorMessage>
                <formulaExpression>OR(
ISBLANK({!Explant_Date}),
AND(
{!Explant_Date} &lt;= {!$Flow.CurrentDate},
{!Explant_Date} &lt;= {!Report_Date}))</formulaExpression>
            </validationRule>
        </fields>
